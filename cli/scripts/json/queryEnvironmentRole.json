{
   "QueryFilter" :
     {
       "expression" :
         {
           "operator" : "and",
           "nestedExpression" : [
             {
               "argument" : ["${envId}"],
               "operator" : "EQUALS",
               "property" : "environmentId"
             },
             {
               "argument" : ["${roleId}"],
               "operator" : "EQUALS",
               "property" : "roleId"
             }
           ]
         }
     }
}
